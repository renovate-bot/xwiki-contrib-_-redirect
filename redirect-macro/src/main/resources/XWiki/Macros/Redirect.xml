    <property>
      <code>let redirectMacroDataDivs = document.getElementsByClassName("redirect-macro-data")
if (redirectMacroDataDivs.length &gt; 0) {
  let doRedirect = true

  // The following code is executed only when CKEditor is loaded on the current page.
  require(['deferred!ckeditor'], function (ckeditorPromise) {
    ckeditorPromise.done(function (ckeditor) {
      // Wait for an editor instance to be created.
      ckeditor.once('instanceCreated', function (event) {
        // cancel redirect if CKEditor is open
        doRedirect = false
      });
    });
  });
  let redirectMacroDataDiv = redirectMacroDataDivs[0]
  let delay = redirectMacroDataDiv.dataset.redirectDelay
  let target = redirectMacroDataDiv.dataset.redirectLocation
  setTimeout(() =&gt; {
    if (doRedirect) {
      window.location = target
    }
  }, delay * 1000)
}</code>
