      <code>{{groovy}}
import org.xwiki.model.reference.DocumentReferenceResolver
import org.xwiki.rendering.syntax.Syntax
import org.xwiki.rendering.syntax.SyntaxType
import org.xwiki.security.authorization.Right
import org.xwiki.url.URLSecurityManager
import org.xwiki.xml.XMLUtils

def logger = services.logging.getLogger("redirect-macro")

// Get the reference parameter
def reference = wikimacro.parameters.reference

// Resolve the reference to a DocumentReference
def resolver = services.component.getInstance(DocumentReferenceResolver.TYPE_STRING, "current")
def documentReference = resolver.resolve(reference.reference)

// Get the delay parameter (default to 0 if not set)
def delay = wikimacro.parameters.delay ?: 0

// Get the visible parameter (default to true if not set)
def visible = wikimacro.parameters.visible != null ? wikimacro.parameters.visible : true

// Determine the target URL
def location
if (xwiki.exists(documentReference)) {
    location = xwiki.getURL(documentReference)
} else {
    location = reference.reference
}

String uri = null

// Check that we are not in spoofing attack case
services.component.getInstance(URLSecurityManager.class)
// Note this code was inspired by
// https://github.com/xwiki/xwiki-platform/blob/8d8ce3b4fc0014ff2e255725e3958f2dc05b0ef7/xwiki-platform-core/xwiki-platform-oldcore/src/main/java/com/xpn/xwiki/web/XWikiServletResponse.java#L56
try {
    uri = services.component.getInstance(URLSecurityManager.class).parseToSafeURI(location)
} catch (URISyntaxException | SecurityException e) {
    logger.warn(
        "Possible phishing attack, attempting to redirect to [{}], this request has been blocked. "
            + "If the request was legitimate, please check the URL security configuration. You "
            + "might need to add the domain related to this request in the list of trusted domains in "
            + "the configuration: it can be configured in xwiki.properties in url.trustedDomains.",
        location)
    logger.debug("Original error preventing the redirect: ", e)
}

if (uri == null) {
    if (services.security.authorization.hasAccess(Right.EDIT)) {
        println """{{error}}
=== Redirect Error ===
You are not allowed redirect to **${services.rendering.escape(location, Syntax.XWIKI_2_1)}** for security reason.
Please contact the administrator to allow the redirection to this website.
{{/error}}

"""
    }
} else {
    // Display warning if visible is true
    if (visible) {
        println """{{warning}}
=== Redirect Notice ===
This page will redirect to **${services.rendering.escape(uri, Syntax.XWIKI_2_1)}** in about **${services.rendering.escape(delay.toString(), Syntax.XWIKI_2_1)}** seconds.
{{/warning}}

"""
    }

    // Set server-side redirect header if in view mode
    Syntax syntax = wikimacro.context.transformationContext.targetSyntax
    SyntaxType targetSyntaxType = syntax == null ? null : syntax.getType()
    if (xcontext.action == 'view' &amp;&amp; SyntaxType.ANNOTATED_HTML != targetSyntaxType &amp;&amp; SyntaxType.ANNOTATED_XHTML != targetSyntaxType) {
        def response = xcontext.response
        if (delay == 0) {
            response.sendRedirect(uri)
        } else {
            xwiki.jsx.use('XWiki.Macros.Redirect')
            println """{{html clean="false"}}&lt;div class="redirect-macro-data" data-redirect-location="${XMLUtils.escape(uri.toString())}" data-redirect-delay="${XMLUtils.escape(delay.toString())}" hidden=""/&gt;{{/html}}"""
        }
    }
}
{{/groovy}}</code>
